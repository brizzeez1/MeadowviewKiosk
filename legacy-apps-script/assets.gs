// ===== Temple 365 App Icon Settings =====
const APP_NAME = "Temple 365";
const APP_SHORT_NAME = "Temple365";
const THEME_COLOR = "#ffffff";
const BG_COLOR = "#ffffff";

// ===== Base64 PNGs (NO line breaks) =====
const APPLE_TOUCH_ICON_180 = "";
const ICON_192 = "";
const ICON_512 = "";
const FAVICON_32 = "";

function dataUrl_(pngBase64) {
  return "data:image/png;base64," + pngBase64;
}

function manifestDataUrl_() {
  const manifest = {
    name: APP_NAME,
    short_name: APP_SHORT_NAME,
    start_url: ".",
    display: "standalone",
    background_color: BG_COLOR,
    theme_color: THEME_COLOR,
    icons: [
      { src: "data:image/png;base64," + ICON_192, sizes: "192x192", type: "image/png" },
      { src: "data:image/png;base64," + ICON_512, sizes: "512x512", type: "image/png" }
    ]
  };

  const json = JSON.stringify(manifest);
  const b64 = Utilities.base64Encode(json);
  return "data:application/manifest+json;base64," + b64;
}
